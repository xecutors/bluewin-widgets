{
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "extends": [
    "airbnb"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/jsx-curly-brace-presence": [0],
    "react/prop-types": [0],
    "jsx-a11y/anchor-is-valid": [0],
    "no-underscore-dangle": [ "error", { "allow": [ "_id" ] } ]
  }
}
